{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://type-blog-frontend.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    },
    {
      "src": "/auth/google/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://type-blog-frontend.vercel.app",
        "Access-Control-Allow-Credentials": "true"
      }
    }
  ]
}
