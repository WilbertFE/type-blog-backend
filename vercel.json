{
  "version": 2,
  "builds": [{ "src": "/server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": ["/api/(.*)", "/auth/google/(.*)"],
      "dest": "/server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://type-blog-frontend.vercel.app/"
      }
    }
  ]
}
